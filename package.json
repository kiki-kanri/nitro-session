{
  "name": "@kikiutils/nitro-session",
  "type": "module",
  "version": "2.0.5",
  "description": "Easy-to-use nitro session.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nitro-session.git"
  },
  "keywords": [
    "nitro",
    "nitro session",
    "nitropack",
    "nitropack session",
    "session"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts"
    }
  },
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "build": "ts-project-builder ./src/index.ts ./src/runtime/utils.ts -f esm --clean --preserve-modules --sourcemaps",
    "build:playground": "nitro build",
    "bumplog": "changelogen --bump",
    "dev": "nitro dev",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "release": "pnpm run lint && pnpm run build && changelogen --push --release && npm publish"
  },
  "dependencies": {
    "consola": "^3.4.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.2",
    "node-ciphers": "2.0.0",
    "on-change": "^5.0.1",
    "unstorage": "^1.15.0"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.7.1",
    "@kikiutils/eslint-config": "^0.8.2",
    "@kikiutils/tsconfigs": "^3.0.2",
    "@types/lodash-es": "^4.17.12",
    "cookie-es": "^2.0.0",
    "h3": "npm:h3-nightly@1.15.1-20250222-111608-d1c00fc",
    "nitro": "npm:nitro-nightly@3.0.0-beta-28796231.359af68d",
    "ts-project-builder": "3.4.1",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
