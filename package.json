{
  "name": "@kikiutils/nitro-session",
  "version": "1.1.2",
  "description": "Easy-to-use nitro session.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nitro-session.git"
  },
  "scripts": {
    "build": "rollup -c --configPlugin typescript=\"{ tsconfig: './tsconfig.rollup.json' }\"",
    "build:playground": "nitropack build",
    "bumplog": "changelogen --bump",
    "dev": "nitro dev"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "node-ciphers": "^0.8.1",
    "on-change": "^5.0.1",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.12",
    "changelogen": "^0.5.5",
    "nitropack": "^2.9.7",
    "rollup": "^4.19.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^7.1.2"
  },
  "keywords": [
    "nitro",
    "nitro session",
    "nitropack",
    "nitropack session",
    "session"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./runtime/*": {
      "import": "./runtime/*.mjs",
      "require": "./runtime/*.cjs",
      "types": "./runtime/*.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
