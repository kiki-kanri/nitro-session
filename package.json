{
  "name": "@kikiutils/nitro-session",
  "version": "0.1.0",
  "description": "Easy-to-use nitro server-side session.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nitro-session.git"
  },
  "type": "module",
  "scripts": {
    "build": "del-cli ./dist && tsc-multi",
    "build:playground": "nitropack build",
    "bumplog": "changelogen --bump",
    "dev": "nitro dev"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "node-ciphers": "^0.8.1",
    "on-change": "^5.0.1",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^2.2.0",
    "@types/lodash-es": "^4.17.12",
    "changelogen": "^0.5.5",
    "del-cli": "^5.1.0",
    "nitropack": "^2.9.7",
    "tsc-multi": "^1.1.0"
  },
  "keywords": [
    "nitro",
    "nitro-session",
    "nitrojs",
    "nitrojs-session",
    "session"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./runtime/*": {
      "import": "./runtime/*.mjs",
      "types": "./runtime/*.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
