{
  "name": "@kikiutils/nitro-session",
  "version": "1.2.8",
  "description": "Easy-to-use nitro session.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nitro-session.git"
  },
  "type": "module",
  "module": "./index.mjs",
  "scripts": {
    "build": "ts-project-builder ./src/index.ts ./src/runtime/utils.ts -f esm --clean --preserve-modules",
    "build:playground": "nitropack build",
    "bumplog": "changelogen --bump",
    "dev": "nitro dev"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "consola": "^3.2.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "node-ciphers": "^0.8.1",
    "on-change": "^5.0.1",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/lodash-es": "^4.17.12",
    "changelogen": "^0.5.5",
    "nitropack": "^2.9.7",
    "ts-project-builder": "3.0.1",
    "tslib": "^2.6.3"
  },
  "keywords": [
    "nitro",
    "nitro session",
    "nitropack",
    "nitropack session",
    "session"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./runtime/*": {
      "import": "./runtime/*.mjs",
      "types": "./runtime/*.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
